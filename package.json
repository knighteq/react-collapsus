{
  "name": "react-collapsus",
  "main": "dist/index.js",
  "version": "0.1.0",
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.0",
    "@babel/preset-env": "7.9.0",
    "@babel/preset-react": "7.9.4",
    "babel-loader": "8.1.0",
    "babel-preset-react-app": "9.1.2",
    "eslint": "6.6.0",
    "eslint-config-react-app": "5.2.1",
    "eslint-plugin-react": "7.19.0",
    "html-webpack-plugin": "4.0.0-beta.11",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "webpack": "4.42.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --mode production --config config/webpack.config.js",
    "storybook": "start-storybook",
    "transpile": "cross-env NODE_ENV=production babel src -d dist --copy-files",
    "prepare": "npm run build",
    "start": "cross-env NODE_ENV=development webpack-dev-server --mode development --open --config config/webpack.config.js"
  },
  "peerDependencies": {
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "lodash": "4.17.15"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "parser": "babel-eslint"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babel": {
    "presets": [
      "react-app",
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "entry"
        }
      ]
    ]
  },
  "dependencies": {
    "cross-env": "7.0.2",
    "lodash": "4.17.15",
    "prop-types": "15.7.2",
    "styled-components": "5.0.1"
  }
}
