{
  "name": "react-collapsus",
  "homepage": "https://knighteq.github.io/react-collapsus",
  "main": "dist/index.js",
  "version": "0.2.1",
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.0",
    "@babel/preset-env": "7.9.0",
    "@babel/preset-react": "7.9.4",
    "@storybook/react": "5.3.18",
    "@types/styled-components": "5.0.1",
    "babel-loader": "8.1.0",
    "babel-preset-react-app": "9.1.2",
    "eslint": "6.6.0",
    "eslint-config-react-app": "5.2.1",
    "eslint-plugin-react": "7.19.0",
    "gh-pages": "2.2.0",
    "html-webpack-plugin": "4.0.0-beta.11",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "source-map-loader": "0.2.4",
    "standard-version": "^7.1.0",
    "ts-loader": "6.2.2",
    "typescript": "3.8.3",
    "webpack": "4.42.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --mode production --config config/webpack.config.js",
    "storybook": "start-storybook",
    "transpile": "cross-env NODE_ENV=production babel src -d dist --copy-files",
    "prepare": "npm run build",
    "start": "cross-env NODE_ENV=development webpack-dev-server --mode development --open --config config/webpack.config.js",
    "release": "standard-version --release-as",
    "build-storybook": "build-storybook -c .storybook -o .out",
    "predeploy": "npm run build-storybook",
    "deploy": "gh-pages -d .out"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/knighteq/react-collapsus.git"
  },
  "peerDependencies": {
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "lodash": "4.17.19"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "parser": "babel-eslint"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babel": {
    "presets": [
      "react-app",
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "entry"
        }
      ]
    ]
  },
  "dependencies": {
    "@types/react": "16.9.34",
    "@types/react-dom": "16.9.6",
    "cross-env": "7.0.2",
    "kind-of": "6.0.3",
    "lodash": "4.17.19",
    "styled-components": "5.0.1"
  }
}
